@model CENV_JMH.DO.Showing

<div>
    <h4>@Model.Name</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Price
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TicketPrice) &euro;
        </dd>
        <dd>
            <img src="@Model.Picture_URL" alt="@Model.Name" width="125" height="170" />
        </dd>
    </dl>
</div>
